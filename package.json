{
  "name": "telemetry",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "prod": "export NODE_PATH=build/ && node build/src/index.js",
    "prod:linux": "export NODE_PATH=build/ && node build/src/index.js",
    "prod:windows": "set NODE_PATH=build/&& node build/src/index.js",
    "start": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "build": "npm run clean && tsc -b",
    "clean": "rimraf build/",
    "test": "jest --coverage",
    "test:watch": "jest --watch --coverage=false",
    "test:debug": "ts-node-dev --transpile-only jest/bin/jest --runInBand --coverage=false"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "cuid": "^2.1.8",
    "dotenv": "^16.0.1",
    "express": "^4.18.2",
    "fp-ts": "^2.13.1",
    "io-ts": "^2.2.19"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/jest": "^28.1.2",
    "@types/node": "^16.18.3",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-functional": "^4.4.1",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.1",
    "prettier": "^2.8.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.5",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.9.4"
  }
}
